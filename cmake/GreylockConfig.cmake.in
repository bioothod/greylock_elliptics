# - Config file for the Elliptics package
# It defines the following variables
#  GREYLOCK_INCLUDE_DIRS - include directories for Elliptics
#  GREYLOCK_LIBRARY_DIRS - library directories
#  GREYLOCK_LIBRARIES    - libraries to link against

get_filename_component(GREYLOCK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

set(GREYLOCK_INCLUDE_DIRS @INSTALL_INCLUDE_DIR@
	@EBUCKET_INCLUDE_DIRS@
	@LZ4_INCLUDE_DIRS@
	@MSGPACK_INCLUDE_DIRS@
	@SWARM_INCLUDE_DIRS@
	@THEVOID_INCLUDE_DIRS@
)

set(GREYLOCK_LIBRARY_DIRS
	@EBUCKET_LIBRARY_DIRS@
	@LZ4_LIBRARY_DIRS@
	@MSGPACK_LIBRARY_DIRS@
	@SWARM_LIBRARY_DIRS@
	@THEVOID_LIBRARY_DIRS@
)

set(GREYLOCK_LIBRARIES
	@EBUCKET_LIBRARIES@
	@LZ4_LIBRARIES@
	@MSGPACK_LIBRARIES@
	@SWARM_LIBRARIES@
	@THEVOID_LIBRARIES@
)
