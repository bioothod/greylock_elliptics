{
    "endpoints": [
        "0.0.0.0:8080"
    ],
    "backlog": 512,
    "threads": 2,
    "buffer_size": 65536,
	"logger": {
        "level": "info",
        "frontends": [
            {
                "formatter": {
                    "type": "string",
                    "pattern": "%(timestamp)s %(request_id)s/%(lwp)s/%(pid)s %(severity)s: %(message)s, %(...L)s"
                },
                "sink": {
                    "type": "files",
                    "path": "/dev/stdout",
                    "autoflush": true,
                    "rotation": { "move": 0 }
                }
            }
        ]
    },
    "daemon": {
        "fork": false,
        "uid": 1000
    },
    "monitor-port": 21235,
    "request_header": "X-Request",
    "trace_header": "X-Trace",
    "application": {
	"remotes": [
		"localhost:1026:2"
	],
	"metadata-groups": [
	    1,2
	],
	"buckets": [
	    "b1",
	    "b2",
	    "b3",
	    "b4",
	    "b5",
	    "b77",
	    "b89",
	    "bucket:12.21",
	    "bucket:22.31",
	    "bucket:32.11"
	],
	"meta-bucket": "b1",
	"lock-num": 1024,
	"max-page-size": 6144,
	"reserve-size": 1536
    }
}
